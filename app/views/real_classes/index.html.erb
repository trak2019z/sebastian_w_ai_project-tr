<p id="notice"><%= notice %></p>

<h1>Real Classes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Year</th>
      <th>Semester</th>
      <th>Active</th>
      <th>Start date</th>
      <th>End date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @real_classes.each do |real_class| %>
      <tr>
        <td><%= real_class.name %></td>
        <td><%= real_class.year %></td>
        <td><%= real_class.semester %></td>
        <td><%= real_class.active %></td>
        <td><%= real_class.start_date %></td>
        <td><%= real_class.end_date %></td>
        <td><%= link_to 'Show', real_class %></td>
        <td><%= link_to 'Edit', edit_real_class_path(real_class) %></td>
        <td><%= link_to 'Destroy', real_class, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Real Class', new_real_class_path %>
